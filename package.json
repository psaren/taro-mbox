{
  "name": "mbox",
  "version": "1.0.0",
  "private": true,
  "description": "a music app",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "cross-env CLIENT_ENV=h5 taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "cross-env CLIENT_ENV=h5 npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "1.3.8",
    "@tarojs/components": "1.3.8",
    "@tarojs/redux": "1.3.8",
    "@tarojs/redux-h5": "1.3.8",
    "@tarojs/router": "1.3.8",
    "@tarojs/taro": "1.3.8",
    "@tarojs/taro-alipay": "1.3.8",
    "@tarojs/taro-h5": "1.3.8",
    "@tarojs/taro-swan": "1.3.8",
    "@tarojs/taro-tt": "1.3.8",
    "@tarojs/taro-weapp": "1.3.8",
    "accounting-js": "^1.1.1",
    "lodash-es": "^4.17.14",
    "nervjs": "^1.4.3",
    "nerv-devtools": "^1.4.3",
    "prop-types": "^15.7.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/webpack-env": "^1.14.0",
    "@tarojs/plugin-babel": "1.3.8",
    "@tarojs/plugin-csso": "1.3.8",
    "@tarojs/plugin-sass": "1.3.8",
    "@tarojs/plugin-uglifyjs": "1.3.8",
    "@tarojs/webpack-runner": "1.3.8",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-taro": "1.3.8",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-taro": "1.3.8"
  }
